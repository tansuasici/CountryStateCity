{
  "name": "@tansuasici/country-state-city",
  "version": "1.0.1",
  "description": "Complete world countries, states, and cities data in JSON, CSV, XML, and YAML formats. Modern UI components included.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "data",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./data/countries.json": "./data/country.json",
    "./data/states.json": "./data/state.json",
    "./data/cities.json": "./data/city.json"
  },
  "scripts": {
    "dev": "env-cmd -f .env.development next dev --turbopack",
    "prebuild:test": "npm version prerelease --preid=test",
    "build:test": "env-cmd -f .env.test next build",
    "postbuild:test": "git add package.json && git commit -m 'Build(test): version bump'",
    "prebuild:prod": "npm version patch",
    "build:prod": "env-cmd -f .env.production next build",
    "postbuild:prod": "git add package.json && git commit -m 'Build(prod): version bump'",
    "build:lib": "tsc --project tsconfig.lib.json",
    "start": "next start",
    "export": "next export",
    "lint": "next lint",
    "build:dev": "SET NODE_ENV=dev npm run build --dev --",
    "build:local": "env-cmd -f .env.local next build",
    "prepublishOnly": "npm run build:lib"
  },
  "keywords": [
    "countries",
    "states",
    "cities",
    "json",
    "csv",
    "xml",
    "yaml",
    "location",
    "geography",
    "world-data",
    "iso3166",
    "nextjs",
    "react",
    "typescript"
  ],
  "author": {
    "name": "Tansu Asici",
    "url": "https://tansuasici.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tansuasici/CountryStateCity"
  },
  "bugs": {
    "url": "https://github.com/tansuasici/CountryStateCity/issues"
  },
  "homepage": "https://countrystatecity.xyz",
  "dependencies": {
    "@heroui/react": "^2.7.8",
    "@types/leaflet": "^1.9.20",
    "autoprefixer": "^10.4.17",
    "firebase": "^12.1.0",
    "framer-motion": "^12.23.12",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "next-themes": "^0.4.6",
    "postcss": "^8.4.33",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "react-leaflet": "^5.0.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "env-cmd": "^10.1.0",
    "typescript": "^5"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  }
}
